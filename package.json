{
	"name": "vsc-extension-stashflow",
	"displayName": "StashFlow",
	"version": "0.0.1",
	"description": "Stash or Rebase Changes upon Switching Branches",
	"main": "src/main.ts",
	"type": "module",
	"scripts": {
		"vscode:prepublish": "pnpm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "pnpm run compile && pnpm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Malix-off/VSC-Extension-StashFlow.git"
	},
	"keywords": [
		"VSC",
		"Visual",
		"Studio",
		"Code",
		"Extension",
		"Git",
		"Stash",
		"Branches",
		"Automatic"
	],
	"author": {
		"name": "Malix",
		"email": "alixbrunetcontact@gmail.com",
		"url": "https://github.com/Malix-off/"
	},
	"bugs": {
		"url": "https://github.com/Malix-off/VSC-Extension-StashFlow/issues"
	},
	"homepage": "https://github.com/Malix-off/VSC-Extension-StashFlow#readme",
	"engines": {
		"vscode": "^1.84.0"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.3",
		"@types/node": "~18.18.8",
		"@types/vscode": "^1.84.0",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"@vscode/test-electron": "^2.3.6",
		"@vscode/vsce": "^2.22.0",
		"eslint": "^8.53.0",
		"glob": "^10.3.10",
		"mocha": "^10.2.0",
		"pnpm": "^8.10.2",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"generator-code": "^1.8.0",
		"yo": "^4.3.1"
	}
